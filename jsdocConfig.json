{
    "source": {
        "include": ["./packages/paypal-js/types/v6/"],
        "includePattern": "\\.(d.ts)$"
    },
    "opts": {
        "destination": "./docs/",
        "recurse": true,
        "readme": "./docs/README.md",
        "template": "./node_modules/better-docs"
    },
    "plugins": ["plugins/markdown", "./node_modules/better-docs/typescript"],
    "markdown": {
        "parser": "gfm"
    }
}
