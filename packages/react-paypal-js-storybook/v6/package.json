    },
    "dependencies": {
        "@paypal/react-paypal-js": "^9.0.0-alpha.6",
        "react": "^19.2.4",
        "react-dom": "^19.2.4"
    },
    "devDependencies": {
        "@storybook/addon-docs": "^10.2.4",
        "@storybook/react": "^10.2.4",
        "@storybook/react-vite": "^10.2.4",
        "storybook": "^10.2.4",
        "typescript": "^5.9.3",
        "vite": "^6.4.1"
    }
