{
  "name": "@paypal/paypal-js",
  "version": "0.1.0",
  "description": "Async loader for the JS SDK",
  "main": "dist/paypal.node.js",
  "module": "dist/paypal.esm.js",
  "scripts": {
    "build": "rm -rf dist && rollup --config",
    "lint": "eslint .",
    "prepublishOnly": "npm run lint && npm test && npm run build && npm run test:bundle && npm run test:e2e",
    "test": "jest --env=jsdom src",
    "test:bundle": "jest --env=jsdom e2e-tests/bundle/**",
    "test:e2e": "jest -c e2e-tests/jest.config.js",
    "test:e2e:start": "http-server"
  },
  "files": [
    "dist"
  ],
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "git://github.com/paypal/paypal-js.git"
  },
  "devDependencies": {
    "@babel/core": "^7.10.4",
    "@babel/preset-env": "^7.10.4",
    "@rollup/plugin-babel": "^5.0.4",
    "@rollup/plugin-commonjs": "^15.0.0",
    "@rollup/plugin-node-resolve": "^9.0.0",
    "@rollup/plugin-replace": "^2.3.3",
    "babel-jest": "^26.1.0",
    "core-js": "^3.6.5",
    "eslint": "^7.4.0",
    "filesize": "^6.1.0",
    "http-server": "^0.12.3",
    "jest": "^26.1.0",
    "jest-puppeteer": "^4.4.0",
    "puppeteer": "^5.1.0",
    "rollup": "^2.21.0",
    "rollup-plugin-filesize": "^9.0.2",
    "rollup-plugin-terser": "^7.0.0"
  },
  "dependencies": {
    "promise-polyfill": "^8.1.3"
  }
